<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>Servlet1</servlet-name>
        <servlet-class>Servlet1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>acuille</servlet-name>
        <servlet-class>acuille</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AjouterCham</servlet-name>
        <servlet-class>com.tp.servlets.AjouterCham</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AjouterCli</servlet-name>
        <servlet-class>com.tp.servlets.AjouterCli</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReserviCham</servlet-name>
        <servlet-class>com.tp.servlets.ReserviCham</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AffichChaLib</servlet-name>
        <servlet-class>com.tp.servlets.AffichChaLib</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NavControl</servlet-name>
        <servlet-class>com.tp.servlets.NavControl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AffiClis</servlet-name>
        <servlet-class>com.tp.servlets.AffiClis</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Servlet1</servlet-name>
        <url-pattern>/auth.html</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>acuille</servlet-name>
        <url-pattern>/acuille</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AjouterCham</servlet-name>
        <url-pattern>/AjouterCham</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AjouterCli</servlet-name>
        <url-pattern>/AjouterCli</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReserviCham</servlet-name>
        <url-pattern>/ReserviCham</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AffichChaLib</servlet-name>
        <url-pattern>/AffichChaLib</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NavControl</servlet-name>
        <url-pattern>/NavControl</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AffiClis</servlet-name>
        <url-pattern>/AffiClis</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <jsp-config>
        <jsp-property-group>
            <url-pattern>*.jsp</url-pattern>
            <include-prelude>/WEB-INF/taglibs.jsp</include-prelude>
        </jsp-property-group>
    </jsp-config>
</web-app>
